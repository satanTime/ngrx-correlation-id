{
  "name": "a12",
  "description": "Angular 12",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:ivy && npm run build:classic",
    "build:ivy": "ng build --configuration ivy",
    "build:classic": "ng build --configuration classic",
    "test": "npm run test:ivy && npm run test:classic",
    "test:ivy": "ng test --configuration ivy",
    "test:classic": "ng test --configuration classic",
    "e2e": "npm run e2e:ivy && npm run e2e:classic",
    "e2e:ivy": "ng e2e --webdriver-update=false --configuration ivy",
    "e2e:classic": "ng e2e --webdriver-update=false --configuration classic",
    "postinstall": "webdriver-manager update --versions.chrome 93.0.4577.63 --gecko=false"
  },
  "peerDependencies": {
    "ngrx-correlation-id": "*"
  },
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@ngrx/effects": "12.5.1",
    "@ngrx/entity": "12.5.1",
    "@ngrx/store": "12.5.1",
    "rxjs": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/cli": "12.2.18",
    "@angular/compiler-cli": "12.2.16",
    "@types/jasmine": "3.6.11",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "12.20.55",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.1.2",
    "karma-chrome-launcher": "3.1.1",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.5.4",
    "npm": "6.14.17",
    "protractor": "7.0.0",
    "puppeteer": "10.4.0",
    "ts-node": "8.3.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "npm": "6.14.17"
  }
}
