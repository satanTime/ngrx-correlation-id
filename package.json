{
    "name": "ngrx-correlation-id",
    "version": "1.0.3",
    "scripts": {
        "ng": "ng",
        "build": "rm -Rf tmp && rm -Rf dist && ng build --prod && cp LICENSE dist/ngrx-correlation-id && cp README.md dist/ngrx-correlation-id",
        "test": "ng test",
        "lint": "ng lint",
        "i:a": "npm run i:a6 && npm run i:a7 && npm run i:a8 && npm run i:a9f && npm run i:a9t && npm run i:a10f && npm run i:a10t && npm run i:a11f && npm run i:a11t",
        "i:a6": "cd e2e/angular6 && npm install && npm run postinstall",
        "i:a7": "cd e2e/angular7 && npm install && npm run postinstall",
        "i:a8": "cd e2e/angular8 && npm install && npm run postinstall",
        "i:a9f": "cd e2e/angular9-ivy-false && npm install && npm run postinstall",
        "i:a9t": "cd e2e/angular9-ivy-true && npm install && npm run postinstall",
        "i:a10f": "cd e2e/angular10-ivy-false && npm install && npm run postinstall",
        "i:a10t": "cd e2e/angular10-ivy-true && npm install && npm run postinstall",
        "i:a11f": "cd e2e/angular11-ivy-false && npm install && npm run postinstall",
        "i:a11t": "cd e2e/angular11-ivy-true && npm install && npm run postinstall",
        "s:a": "npm run s:a6 && npm run s:a7 && npm run s:a8 && npm run s:a9f && npm run s:a9t && npm run s:a10f && npm run s:a10t && npm run s:a11f && npm run s:a11t",
        "s:a6": "P=e2e/angular6/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a7": "P=e2e/angular7/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a8": "P=e2e/angular8/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a9f": "P=e2e/angular9-ivy-false/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a9t": "P=e2e/angular9-ivy-true/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a10f": "P=e2e/angular10-ivy-false/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a10t": "P=e2e/angular10-ivy-true/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a11f": "P=e2e/angular11-ivy-false/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "s:a11t": "P=e2e/angular11-ivy-true/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "b:a": "npm run b:a6 && npm run b:a7 && npm run b:a8 && npm run b:a9f && npm run b:a9t && npm run b:a10f && npm run b:a10t && npm run b:a11f && npm run b:a11t",
        "b:a6": "cd e2e/angular6 && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a7": "cd e2e/angular7 && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a8": "cd e2e/angular8 && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a9f": "cd e2e/angular9-ivy-false && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a9t": "cd e2e/angular9-ivy-true && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a10f": "cd e2e/angular10-ivy-false && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a10t": "cd e2e/angular10-ivy-true && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a11f": "cd e2e/angular11-ivy-false && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a11t": "cd e2e/angular11-ivy-true && npm run build -- --prod --progress=false && rm -Rf dist",
        "e2e:a": "npm run e2e:a6 && npm run e2e:a7 && npm run e2e:a8 && npm run e2e:a9f && npm run e2e:a9t && npm run e2e:a10f && npm run e2e:a10t && npm run e2e:a11f && npm run e2e:a11t",
        "e2e:a6": "cd e2e/angular6 && npm run e2e",
        "e2e:a7": "cd e2e/angular7 && npm run e2e",
        "e2e:a8": "cd e2e/angular8 && npm run e2e",
        "e2e:a9f": "cd e2e/angular9-ivy-false && npm run e2e",
        "e2e:a9t": "cd e2e/angular9-ivy-true && npm run e2e",
        "e2e:a10f": "cd e2e/angular10-ivy-false && npm run e2e",
        "e2e:a10t": "cd e2e/angular10-ivy-true && npm run e2e",
        "e2e:a11f": "cd e2e/angular11-ivy-false && npm run e2e",
        "e2e:a11t": "cd e2e/angular11-ivy-true && npm run e2e",
        "e2e": "npm run build && npm run i:a && npm run s:a && npm run e2e:a",
        "test:cid": "ng test ngrx-correlation-id",
        "test:cid:debug": "ng test ngrx-correlation-id -- --watch --browsers=Chrome",
        "publish": "cd dist/ngrx-correlation-id && npm publish",
        "release": "export $(cat .env) && semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:satanTime/ngrx-correlation-id.git"
    },
    "author": "MG. <m@sudo.eu>",
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/common": "10.2.4",
        "@angular/compiler": "10.2.4",
        "@angular/core": "10.2.4",
        "@angular/platform-browser": "10.2.4",
        "@angular/platform-browser-dynamic": "10.2.4",
        "@ngrx/effects": "10.1.2",
        "@ngrx/entity": "10.1.2",
        "@ngrx/store": "10.1.2",
        "core-js": "2.6.12",
        "rxjs": "6.6.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1002.1",
        "@angular-devkit/build-ng-packagr": "~0.1002.1",
        "@angular-devkit/core": "^11.0.5",
        "@angular/cli": "10.2.1",
        "@angular/compiler-cli": "10.2.4",
        "@angular/language-service": "10.2.4",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/exec": "5.0.0",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/github": "7.2.0",
        "@types/jasmine": "3.5.11",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "^12.11.1",
        "codecov": "3.8.1",
        "codelyzer": "6.0.1",
        "commitlint": "8.3.5",
        "coveralls": "3.1.0",
        "husky": "4.3.6",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "karma-junit-reporter": "2.0.1",
        "lint-staged": "10.5.3",
        "ng-packagr": "^10.1.0",
        "prettier": "2.2.1",
        "protractor": "~7.0.0",
        "puppeteer": "5.4.1",
        "rxjs-tslint-rules": "4.34.7",
        "semantic-release": "17.3.0",
        "ts-node": "8.10.2",
        "tslint": "~6.1.0",
        "tslint-defocus": "2.0.6",
        "tslint-lines-between-class-members": "1.3.6",
        "tslint-microsoft-contrib": "6.2.0",
        "typescript": "4.0.5"
    },
    "ngPackage": {
        "assets": [
            "CHANGELOG.md",
            "LICENSE",
            "README.md"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged && npm run lint && npm run test",
            "post-commit": "git update-index --again"
        }
    },
    "renovate": {
        "extends": [
            "config:base",
            ":semanticCommitTypeAll(chore)"
        ],
        "rangeStrategy": "pin",
        "rebaseWhen": "conflicted",
        "separateMajorMinor": true,
        "separateMinorPatch": true,
        "separateMultipleMajor": true,
        "ignoreDeprecated": false,
        "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
        "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
        "packageRules": [
            {
                "paths": [
                    "projects/ngrx-correlation-id/package.json"
                ],
                "depTypeList": [
                    "peerDependencies"
                ],
                "enabled": false
            },
            {
                "packageNames": [
                    "puppeteer"
                ],
                "groupName": "puppeteer",
                "groupSlug": "puppeteer"
            }
        ]
    }
}
